<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Year_is_Correct</fullName>
    <active>true</active>
    <errorConditionFormula>Year__c &lt; 1950  ||  Year__c &gt; YEAR(TODAY())</errorConditionFormula>
    <errorDisplayField>Year__c</errorDisplayField>
    <errorMessage>Year value should be between 1950 and current year</errorMessage>
</ValidationRule>
